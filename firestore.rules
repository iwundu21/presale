rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // This rule allows anyone with your project's credentials to read and write
    // to your Firestore database. This is useful for getting started, but it's
    // important to secure your database once you're ready to launch.
    // To learn more about Firestore security rules, check out the documentation:
    // https://firebase.google.com/docs/firestore/security/get-started
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
